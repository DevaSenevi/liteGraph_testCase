},"source":"src/index.js","original":{"line":70,"column":32}},{"generated":{"line":65,"column":33},"source":"src/index.js","original":{"line":70,"column":33}},{"generated":{"line":65,"column":34},"source":"src/index.js","original":{"line":70,"column":34}},{"generated":{"line":67,"column":0},"source":"src/index.js","original":{"line":72,"column":0}},{"generated":{"line":68,"column":0},"source":"src/index.js","original":{"line":73,"column":0},"name":"graph"},{"generated":{"line":68,"column":5},"source":"src/index.js","original":{"line":73,"column":5}},{"generated":{"line":68,"column":6},"source":"src/index.js","original":{"line":73,"column":6},"name":"start"},{"generated":{"line":68,"column":11},"source":"src/index.js","original":{"line":73,"column":11}},{"generated":{"line":68,"column":13},"source":"src/index.js","original":{"line":73,"column":13}}],"sources":{"src/index.js":"//An example script to create a simple sum calculation workflow\n\nimport { LiteGraph, LGraph, LGraphCanvas } from \"litegraph.js\";\nimport \"/litegraph.css\";\nimport \"./styles.css\";\n\nvar graph = new LGraph();\n\nvar canvas = new LGraphCanvas(\"#mycanvas\", graph);\n\n//Create a custom node for calc sum\n\n//node constructor\nfunction addNode(){\n    this.addInput(\"A\", \"number\");\n    this.addInput(\"B\",\"number\");\n    this.addOutput(\"A+B\",\"number\");\n    this.properties = {precision:1};\n}\n\n//Name\naddNode.title = \"sum\"\n\n\n//function called when a sum node is executed\naddNode.prototype.onExecute = function()\n{\n    var A = this.getInputData(0);\n    if( A == undefined)\n        A=0;\n    \n    var B = this.getInputData(1);\n    if( B == undefined)\n        B=0;\n    \n    this.setOutputData( 0, A + B );      \n    \n}\n\n//register the sum node on the system under the basic category\nLiteGraph.registerNodeType(\"basic/sum\", addNode);\n\n/////////////////////////////////////////////////////////////\n\n//Add a const number node --> lives inside basic const category\nvar node_const_one = LiteGraph.createNode(\"basic/const\");\nnode_const_one.pos = [200, 200]; //postion on the graph\ngraph.add(node_const_one); //Add to graph \nnode_const_one.setValue(3); //Set an initial value\n\n//Add a const number node\nvar node_const_two = LiteGraph.createNode(\"basic/const\");\nnode_const_two.pos = [200,400];\ngraph.add(node_const_two);\nnode_const_two.setValue(5);\n\n//Add a sum node\nvar node_sum = LiteGraph.createNode(\"basic/sum\");\nnode_sum.pos = [600,300];\ngraph.add(node_sum);\n\n//Add a watch node\nvar node_watch = LiteGraph.createNode(\"basic/watch\");\nnode_watch.pos =[800,320];\ngraph.add(node_watch);\n\n//create the node links\nnode_const_one.connect(0, node_sum, 0);\nnode_const_two.connect(0, node_sum, 1);\nnode_sum.connect(0, node_watch, 0);\n\n//render the graph\ngraph.start();\n\n"},"lineCount":null}},"error":null,"hash":"a4f8f6370bbc9234a51ca60fcd3315fa","cacheData":{"env":{}}}